<h3>New Asset</h3>

<%= form_for @asset do |f| %>
<!-- Get AssetType id -->
<%= collection_select(:asset, :asset_type_id, AssetType.all, :id, :title) %><br>
<!-- Get User id -->
<%= collection_select(:asset, :user_id, User.all, :id, :name) %>
<%= f.submit %>
<% end %>


