<%= render "shared/navigation_top" %>
<h3>Assets</h3>
<!-- show all Asset -->
<%= div_for(@assets) do |asset| %>
      <span>Title: <%= asset.title || "none" %></span><br>
      <span>Type: <strong><%= asset.asset_type.title %></strong></span><br>
      <span>User: <em><%= asset.user.name %></em></span><br>
      <span>Asset Tag: <em><%= asset.asset_tag %></em></span>
<% end %>

<%= link_to "New Asset", new_asset_path %> &nbsp; 
<%= link_to "New Type", new_asset_type_path %> &nbsp;
